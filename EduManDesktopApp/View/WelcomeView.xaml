<UserControl x:Class="EduManDesktopApp.View.WelcomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EduManDesktopApp.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock Text="Sử dụng nội bộ" HorizontalAlignment="Center"></TextBlock>
            <Image Source="EduManDesktopApp;component\Assets\images\applogo.png"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Height="200">
            </Image>
        </StackPanel>        
    </Grid>
</UserControl>
